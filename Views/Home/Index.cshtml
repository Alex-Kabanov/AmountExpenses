@model IEnumerable<Expense>
@{

}
<table class="table">
    <tr>
        <th>Title</th>
        <th>Amount</th>
        <th>Month</th>
        <th>Categories</th>
        <th>Edit</th>
        <th>Remove</th>
    </tr>
    @foreach (var el in Model)
    {
        <tr>
            <a asp-controller="Home" asp-action="Post" asp-route-id="@el.Id"></a>   
            <th>@el.Title</th>
            <th>@el.Amount</th>
            <th>@el.Month</th>
            <th>@el.Categories</th>
            <th><a asp-controller="Home" asp-action="Edit" asp-route-id="@el.Id">Edit</a></th>
            <th><a asp-controller="Home" asp-action="Remove" asp-route-id="@el.Id">Remove</a></th>
        </tr>

    }

</table>